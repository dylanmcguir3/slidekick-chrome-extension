{
    "manifest_version": 3,
    "name": "Slidekick",
    "version": "1.0",
    "description": "Next Slide Generation",
    "permissions": ["scripting", "activeTab"],
    "icons": {
      "16": "slidekick-logo.png",
      "32": "slidekick-logo.png",
      "48": "slidekick-logo.png",
      "128": "slidekick-logo.png"
    },
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ]
  }